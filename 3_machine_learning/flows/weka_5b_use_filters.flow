adams.flow.control.Flow
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.WekaClassifierSetup
					-classifier
						weka.classifiers.trees.J48
							-C
							0.25
							-M
							2
			-actor
				adams.flow.sink.HistoryDisplay
					-entry-name-variable
					entry
	-actor
		adams.flow.source.SelectFile
			-annotation
			load anneal
			-stop-if-canceled
			true
			-custom-stop-message
			Select train ARFF file
			-extension
			arff
	-actor
		adams.flow.transformer.WekaFileReader
			-loader
				weka.core.converters.AArffLoader
	-actor
		adams.flow.transformer.WekaClassSelector
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					full
					-actor
						adams.flow.transformer.WekaCrossValidationEvaluator
							-output
								weka.classifiers.evaluation.output.prediction.Null
					-actor
						adams.flow.transformer.WekaEvaluationSummary
					-actor
						adams.flow.transformer.SetVariable
							-var-name
							entry
							-var-value
							full
					-actor
						adams.flow.sink.CallableSink
							-callable
							HistoryDisplay
			-branch
				adams.flow.control.Sequence
					-name
					nominal
					-actor
						adams.flow.transformer.WekaFilter
							-filter
								weka.filters.unsupervised.attribute.RemoveType
									-T
									numeric
					-actor
						adams.flow.transformer.WekaCrossValidationEvaluator
							-output
								weka.classifiers.evaluation.output.prediction.Null
					-actor
						adams.flow.transformer.WekaEvaluationSummary
					-actor
						adams.flow.transformer.SetVariable
							-var-name
							entry
							-var-value
							nominal
					-actor
						adams.flow.sink.CallableSink
							-callable
							HistoryDisplay
			-branch
				adams.flow.control.Sequence
					-name
					numeric
					-actor
						adams.flow.transformer.WekaFilter
							-filter
								weka.filters.unsupervised.attribute.RemoveType
									-T
									nominal
					-actor
						adams.flow.transformer.WekaCrossValidationEvaluator
							-output
								weka.classifiers.evaluation.output.prediction.Null
					-actor
						adams.flow.transformer.WekaEvaluationSummary
					-actor
						adams.flow.transformer.SetVariable
							-var-name
							entry
							-var-value
							numeric
					-actor
						adams.flow.sink.CallableSink
							-callable
							HistoryDisplay
			-num-threads
			1
	-flow-execution-listener
		adams.flow.execution.NullListener

