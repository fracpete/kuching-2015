adams.flow.control.Flow
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.sink.SimplePlot
					-writer
						adams.gui.print.NullWriter
					-paintlet
						adams.gui.visualization.sequence.LinePaintlet
					-overlay-paintlet
						adams.gui.visualization.sequence.NullPaintlet
					-mouse-click-action
						adams.flow.sink.sequenceplotter.NullClickAction
					-color-provider
						adams.gui.visualization.core.DefaultColorProvider
					-axis-x
						adams.gui.visualization.core.AxisPanelOptions
							-label
							x
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
									-num-ticks
									20
							-nth-value
							2
							-width
							40
							-custom-format
							0.0
					-axis-y
						adams.gui.visualization.core.AxisPanelOptions
							-label
							y
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							2
							-width
							60
							-custom-format
							0.0
	-actor
		adams.flow.standalone.SetVariable
			-name
			formula template
			-var-name
			formula
			-var-value
			1/(@{stdev}*sqrt(2*pi))*pow(e, (-pow((X-@{mean}), 2)/(2*pow(@{stdev},2))))
	-actor
		adams.flow.source.ForLoop
			-lower
			-100
			-upper
			100
	-actor
		adams.flow.transformer.MathExpression
			-expression
			X/10
	-actor
		adams.flow.transformer.SetVariable
			-var-name
			X
	-actor
		adams.flow.control.Trigger
			-name
			iterate means
			-tee
				adams.flow.source.StringConstants
					-string
					0
					-string
					1
					-string
					-2.2
			-tee
				adams.flow.transformer.SetVariable
					-var-name
					mean
			-tee
				adams.flow.control.Trigger
					-name
					iterate stdevs
					-tee
						adams.flow.source.StringConstants
							-string
							1
							-string
							3
					-tee
						adams.flow.transformer.SetVariable
							-var-name
							stdev
					-tee
						adams.flow.control.Trigger
							-name
							plot name
							-tee
								adams.flow.source.CombineVariables
									-expression
									@{mean}/@{stdev}
							-tee
								adams.flow.transformer.SetVariable
									-var-name
									plotname
					-tee
						adams.flow.control.Trigger
							-name
							calculate plot
							-tee
								adams.flow.source.Variable
									-var-name
									X
							-tee
								adams.flow.transformer.Convert
									-conversion
										adams.data.conversion.StringToDouble
							-tee
								adams.flow.transformer.MathExpression
									-expression
									@{formula}
									-output-value-pair
									true
							-tee
								adams.flow.transformer.MakePlotContainer
									-plot-name
									@{plotname}
							-tee
								adams.flow.sink.CallableSink
									-callable
									SimplePlot
	-flow-execution-listener
		adams.flow.execution.NullListener

